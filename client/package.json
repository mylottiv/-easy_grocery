    "build": "react-scripts build",
    "test": "react-scripts test",
    "prepare": "cd .. && husky install client/.husky",
    "eject": "react-scripts eject",
    "eslint": "eslint",
    "prettier": "prettier"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "resolutions": {
